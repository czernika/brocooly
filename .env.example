# Database
DB_NAME=dbname
DB_USER=root
DB_PASSWORD=

# Optional DB params
DB_HOST=localhost
DB_PREFIX=wp_

# Optionally, you can use a data source name (DSN)
# When using a DSN, you can remove the DB_NAME, DB_USER, DB_PASSWORD, and DB_HOST variables
# DATABASE_URL='mysql://database_user:database_password@database_host:database_port/database_name'
IP=127.0.0.1
DOMAIN=

WP_ENV=development
WP_HOME="http://${DOMAIN}"
WP_SITEURL="${WP_HOME}/wp"

# Folder inside Brocooly theme where all logging files will be stored.
# Set to false if you don't want to
WP_DEBUG_LOG='storage/logs'

# Generate your keys here: https://roots.io/salts.html
AUTH_KEY='key'
SECURE_AUTH_KEY='key'
LOGGED_IN_KEY='key'
NONCE_KEY='key'
AUTH_SALT='key'
SECURE_AUTH_SALT='key'
LOGGED_IN_SALT='key'
NONCE_SALT='key'

# Maintanence mode
MAINTENANCE_MODE=false
MAINTENANCE_TEMPLATE='maintenance.twig'

# Theme
THEME=brocooly
PUBLIC_FOLDER=public

BROWSER=firefox
MANIFEST='manifest.json'
CONCAT_SCRIPTS_IN_PRODUCTION=true